{
  "name": "payara-script",
  "version": "1.0.0",
  "description": "A utility package for managing Payara Server versions.",
  "bin": {
    "payara": "lib/index.js",
    "asadmin": "lib/asadmin.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && babel src -d lib",
    "watch": "babel src -d lib --watch",
    "deploy": "npm run clean && npm install && npm run build && npm publish"
  },
  "keywords": [
    "cli",
    "Payara"
  ],
  "author": "Matt Gill",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/MattGill98/payara-script.git"
  },
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@types/yargs": "^13.0.3",
    "configstore": "^5.0.0",
    "eslint": "^6.5.1",
    "find-process": "^1.4.2",
    "mvn-artifact-url": "^4.0.0",
    "ncp": "^2.0.0",
    "progress-extract": "^1.0.2",
    "regenerator-runtime": "^0.13.3",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "tail": "^2.0.3",
    "yargs": "^14.0.0"
  }
}
